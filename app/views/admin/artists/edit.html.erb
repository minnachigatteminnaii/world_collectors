<div class="row">
    <%= form_for(@artist, url: admin_artist_path) do |f| %>
        <div class="form-group">
            <%= f.label :name, "アーティスト名" %>
            <%= f.text_field :name, class:"form-control" %>
        </div>
        <%= f.submit '編集', class:"btn btn-primary" %>
    <% end %>
</div>
