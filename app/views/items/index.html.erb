<div class="row">
    <div class="col-md-3">

    </div>

    <div class="col-md-9">
        <% @items.each do |item| %>
            <tr>
                <td><%= attachment_image_tag item, :jacket_image, :fill, 50, 50 %></td>
                <td><%= item.name%></td>
                <td><%= item.label_name%></td>
                <td><%= item.price%></td>
                <td><%= link_to "link", item_show_path(item.id) %></td>

            </tr>
        <% end %>
    </div>
</div>